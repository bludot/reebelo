######################
# NestJS
######################
ENV=local
PORT=3000
SERVICE_VERSION=1.0.0
HOSTNAME=0.0.0.0
NESTJS_LOGS_ENABLED=true
LOG_LEVEL=debug

######################
# Postgres
######################
DBHOST=localhost
DBPORT=3306
DBUSERNAME=auth
DBPASSWORD=mysecretpassword
DBDATABASE=test
NODE_TLS_REJECT_UNAUTHORIZED=0

######################
# Document System
######################
FS_ROOT=/storage
FS_DEFAULT_SIZE=100M


######################
# JWT Config
######################
JWT_SECRET=secret
JWT_EXPIRES_IN=600s

######################
# RefreshToken Config
######################
TOKEN_EXPIRATION_DAYS=30
